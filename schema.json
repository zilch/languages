{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "Languages",
  "patternProperties": {
    "^.*$": {
      "type": "object",
      "properties": {
        "name": {
            "type": "string"
        },
        "icon": {
            "type": "string"
        }
      },
      "required": ["name"],
      "additionalProperties": false
    }
  },
  "properties": {
    "$schema": { "type": "string" }
  },
  "additionalProperties": false,
  "type": "object"
}
